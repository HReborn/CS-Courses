package InsertionSort;

public class InsertionSort {
	public int[] sort(int[] arr) {
		for (int limiter=0; limiter < arr.length; limiter++) {
			int i = 1;
			int j = limiter;
			int border = arr[limiter];
			while (border < arr[limiter-i]) {
				arr[j] = arr[j-1];
				i++;
				j--;
			}
			
		}
		
		return null;
	}
}
